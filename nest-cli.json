{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/mini-angles-backend/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/mini-angles-backend/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/mini-angles-backend",
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "loan-management": {
      "type": "application",
      "root": "apps/loan-management",
      "entryFile": "main",
      "sourceRoot": "apps/loan-management/src",
      "compilerOptions": {
        "tsConfigPath": "apps/loan-management/tsconfig.app.json"
      }
    },
    "mini-angles-backend": {
      "type": "application",
      "root": "apps/mini-angles-backend",
      "entryFile": "main",
      "sourceRoot": "apps/mini-angles-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mini-angles-backend/tsconfig.app.json"
      }
    },
    "money-management": {
      "type": "application",
      "root": "apps/money-management",
      "entryFile": "main",
      "sourceRoot": "apps/money-management/src",
      "compilerOptions": {
        "tsConfigPath": "apps/money-management/tsconfig.app.json"
      }
    }
  }
}